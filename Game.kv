<GameWindow>:
    input: my_input
    guess_form_name: ""
    number_of_tips: 0
    FloatLayout:
        Label:
            text: root.automaton_text
            size_hint: .8, .3
            pos_hint: {'x': .1, 'y': .55}
        Label:
            text: root.answer_text
            size_hint: .5, .2
            pos_hint: {'x': .1, 'y': .30}
        TextInput:
            id: my_input
            size_hint: .5, .2
            pos_hint: {'x': 0, 'y': 0}
        Button:
            text: root.button_text
            size_hint: .4, .2
            pos_hint: {'x': .6, 'y': 0}
            on_press: root.check_word()
        Button:
            text: "Main Menu"
            size_hint: .4, .2
            pos_hint: {'x': .6, 'y': .8}
            on_release: root.go_to_main_menu()
        Button:
            text: "Equivalence Check..."
            size_hint: .4, .2
            pos_hint: {'x': 0, 'y': .8}
            on_release: root.go_to_guess_form() 

