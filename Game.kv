<MainWindow>:
    name: "main"
    GridLayout:
        cols: 2
        spacing: 20

        Button:
            default_size_values: .2, .2
            size_hint: .2, .2
            text: "Guess DFA"
            color: 1, 0, 0, 1
            background_normal: "dfa.jpg"
            on_release:
                app.root.current = "dfa_choose"
                root.manager.transition.direction = "right"

        Button:
            default_size_values: .2, .2
            size_hint: .2, .2
            text: "Guess VPA"
            color: 1, 0, 0, 1
            background_normal: "vpa.jpg"
            on_release:
                app.root.current = "vpa_choose"
                root.manager.transition.direction = "right"

        Button:
            default_size_values: .2, .2
            size_hint: .2, .2
            text: "Guess WFA"
            color: 1, 0, 0, 1
            background_normal: "wfa.png"
            on_release:
                app.root.current = "wfa_choose"
                root.manager.transition.direction = "right"

        Button:
            default_size_values: .2, .2
            size_hint: .2, .2
            text: "Exit game"
            color: 1, 0, 0, 1
            background_normal: "exit.jpg"
            on_release: app.stop() 

<DFAChooseVersionWindow>:
    name: "dfa_choose"
    GridLayout:
        cols: 1

        GridLayout:
            cols: 2

            Button:
                default_size_values: .2, .2
                size_hint: .2, .2
                text: "Start a DFA game v1"
                on_release:
                    root.go_to_new_game("game_dfa1_lvl1")

            Button:
                default_size_values: .2, .2
                size_hint: .2, .2
                text: "Start a DFA game v2"
                on_release:
                    root.go_to_new_game("game_dfa2_lvl1")
        Button:
            text: "Main Menu"
            default_size_values: .4, .2
            size_hint: .4, .2
            pos_hint: {'x': .6, 'y': .8}
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "right"

